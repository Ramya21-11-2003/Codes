package main;
import java.util.*;
public class RecuFact {
	public static void main(String [] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter number ");
		int n = scan.nextInt();
		int res = fact(n);
		System.out.println(res);
	}
	static int fact(int n)
	{
		if(n==1)
		{
			return 1;
		}
		else
		{
			n = n * fact(n-1);
			return n;
			
		}
	}

}
